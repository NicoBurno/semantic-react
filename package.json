{
  "name": "semantic-react",
  "version": "0.0.2",
  "description": "React components for Semantic UI.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "webpack --colors --progress --display-modules  --config webpack-production.config.js",
    "test": "./node_modules/karma/bin/karma start --single-run --no-auto-watch karma.config.js",
    "test-watch": "./node_modules/karma/bin/karma start --auto-watch --no-single-run karma.config.js"
  },
  "keywords": [
    "react",
    "semantic",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hallister/semantic-react.git"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "setupEnvScriptFile": "<rootDir>/jestenv.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "author": "Justin Hall <justin@hallister.me>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.1.3",
    "raf": "^3.1.0",
    "react": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1",
    "react-motion": "^0.2.7",
    "react-outsideclickhandler": "^0.1.0",
    "tween-interpolate": "^1.0.1"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.0.5",
    "babel-runtime": "^5.8.20",
    "eslint": "^1.4.3",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.3.2",
    "express": "^4.13.3",
    "he": "^0.5.0",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jasmine-core": "^2.3.4",
    "json-loader": "^0.5.2",
    "karma": "^0.13.9",
    "karma-coverage": "^0.5.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "react-addons-perf": "^0.14.0-rc1",
    "react-addons-test-utils": "^0.14.0-rc1",
    "react-transform-catch-errors": "^0.2.0",
    "react-transform-webpack-hmr": "^0.1.5",
    "redbox-react": "^1.0.5",
    "uglify-js": "^2.4.24",
    "webpack": "^1.11.0",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.10.1",
    "webpack-hot-middleware": "^2.1.0"
  }
}
