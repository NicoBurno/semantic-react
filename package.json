{
  "name": "semantic-react",
  "version": "0.0.2",
  "description": "React components for Semantic UI.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "webpack --colors --progress --display-modules  --config webpack-production.config.js",
    "test": "NODE_ENV=test ./node_modules/karma/bin/karma start --single-run --no-auto-watch karma.config.js",
    "test-watch": "NODE_ENV=test ./node_modules/karma/bin/karma start --auto-watch --no-single-run karma.config.js"
  },
  "keywords": [
    "react",
    "semantic",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hallister/semantic-react.git"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "setupEnvScriptFile": "<rootDir>/jestenv.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "author": "Justin Hall <justin@hallister.me>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.1.3",
    "raf": "^3.1.0",
    "react-addons-transition-group": "^0.14.0",
    "react-outsideclickhandler": "^0.1.0",
    "tween-interpolate": "^1.0.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.25",
    "chai": "^3.3.0",
    "eslint": "^1.5.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.4.2",
    "express": "^4.13.3",
    "he": "^0.5.0",
    "isparta-loader": "^1.0.0",
    "istanbul-react": "^1.1.0",
    "js-logger": "^1.2.0",
    "json-loader": "^0.5.3",
    "karma": "^0.13.10",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.2",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "lodash": "^3.10.1",
    "mocha": "^2.3.3",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "react": "^0.14.0",
    "react-addons-perf": "^0.14.0",
    "react-addons-test-utils": "^0.14.0",
    "react-debug": "^1.0.0",
    "react-docgen": "^2.1.2",
    "react-dom": "^0.14.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.1.1",
    "sinon": "^1.17.0",
    "skin-deep": "^0.8.1",
    "strip-loader": "^0.1.0",
    "uglify-js": "^2.4.24",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  }
}
